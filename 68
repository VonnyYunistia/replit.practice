public static void main (String[]  args){

 char symbol='$';
  
    
    for(int i=0; i<4; i++ ){
      
      System.out.print(symbol);
    }
    
 System.out.println();

    for(int i=1; i<2; i++ ){
      
      System.out.print(symbol+" "+" "+symbol);
    }
    System.out.println();

    for(int i=1; i<2; i++ ){
      
      System.out.print(symbol+" "+" "+symbol);
    }
    System.out.println();

    for(int i=0; i<4; i++ ){
      
      System.out.print(symbol);
    }
    
    
  }
